{% extends 'admin/base.html.twig' %}

{% block title %} Création catégorie ressources {% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <div class="col-md-6 text-center" style="background:#578ec9; border-radius:5px; color:white;">   
            <h4>Nouvelle catégorie de fonds</h4>
        </div>         
    </div>
    <br>
    <div>
        <div class="col-md-6">
            {{form_start(form)}}
            {{form_widget(form)}}
            <button type="submit" class="btn btn-success">Enregister</button>
            {{form_end(form)}}
        </div>  
    </div>   
</div>
{% endblock %}